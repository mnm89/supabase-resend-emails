{
    "compilerOptions": {
        "baseUrl": ".",
        "strict": true,
        "jsx": "react-jsx",
        "paths": {
            "@emails-config": [
                "emails/config.ts"
            ],
            "@emails-footer": [
                "emails/shared/footer.tsx"
            ],
            "@emails-header": [
                "emails/shared/header.tsx"
            ],
            "@emails-signature": [
                "emails/shared/signature.tsx"
            ],
